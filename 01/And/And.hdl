/**
 * And gate: 
 * out = 1 if (a == 1 and b == 1)
 *       0 otherwise
 */

CHIP And {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
	//2 nands in series i.e. 2not gates neutralizing effects of each other and resulting into NAND
	Nand(a=a,b=b,out=abnand);
	Nand(a=abnand,b=abnand,out=out);
}
