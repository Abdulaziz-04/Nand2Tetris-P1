/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
	Not(in=sel,out=Notsel);
	And(a=a,b=Notsel,out=aAndNots);
	And(a=b,b=sel,out=bAnds);
	Or(a=aAndNots,b=bAnds,out=out);
	
}